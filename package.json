{
	"name": "quickmark",
	"version": "1.0.0",
	"description": "A minimalist, self-hosted bookmarking service designed to be your default new tab page",
	"author": "drkpxl",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/drkpxl/quickmark.git"
	},
	"bugs": {
		"url": "https://github.com/drkpxl/quickmark/issues"
	},
	"homepage": "https://github.com/drkpxl/quickmark#readme",
	"keywords": [
		"bookmarks",
		"sveltekit",
		"self-hosted",
		"privacy",
		"sqlite",
		"docker"
	],
	"scripts": {
		"dev": "vite dev --port 9022",
		"build": "vite build",
		"preview": "vite preview --port 9022",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.3.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/better-sqlite3": "^7.6.11",
		"svelte": "^5.11.2",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"better-sqlite3": "^11.0.0",
		"bootstrap": "^5.3.3",
		"bootstrap-icons": "^1.13.1",
		"jsdom": "^25.0.0"
	},
	"type": "module"
}
